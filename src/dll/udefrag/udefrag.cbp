				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="/MD" />
					<Add option="/Ox" />
					<Add option="/W3" />
					<Add option="/nologo /W3 /GS- /Gd /O2 /c" />
					<Add option="/DBUILD_DLL" />
					<Add option="/DWIN32" />
